{
  "_version": "1.33.0",
  "sap.app": {
    "id": "sap.nexus.zdewmo",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZDEMO_PAPHANG_SRV",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "mocks": {
        "uri": "model/mocks.json",
        "type": "JSON"
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "sap.nexus.zdewmo.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.100.2",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.f": {},
        "sap.uxap": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sap.nexus.zdewmo.i18n.i18n",
          "supportedLocales": [""],
          "fallbackLocale": ""
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true
      },
      "mocksModel": {
        "type": "sap.ui.model.json.JSONModel",
        "dataSource": "mocks"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "viewPath": "sap.nexus.zdewmo.view",
        "controlId": "layout",
        "transition": "slide",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "home",
          "target": ["home"]
        },
        {
          "pattern": "item",
          "name": "item",
          "target": ["targetitem"]
        },
        {
          "pattern": "detail/{vbeln}/{posnr}",
          "name": "detail",
          "target": ["tgt_detail"]
        }
      ],
      "targets": {
        "home": {
          "viewName": "Home",
          "controlAggregation": "beginColumnPages"
        },
        "targetitem": 
        {
          "viewName": "Item",
          "controlAggregation": "beginColumnPages"
        },
        "tgt_detail":
        {
          "viewName": "Detail",
          "controlAggregation": "beginColumnPages"
        }
        
      }
    }
  }
}
